          },
          "category": {
            "type": "string",
            "example": "category_id"
          },
          "description": {
            "type": "string",
            "example": "Acara - description"
          },
          "startDate": {
            "type": "string",
            "example": "2024-12-16 10:45:00"
          },
          "endDate": {
            "type": "string",
            "example": "2024-12-16 12:45:00"
          },
          "location": {
            "type": "object",
            "properties": {
              "region": {
                "type": "number",
                "example": 3213180003
              },
              "coordinates": {
                "type": "array",
                "example": [
                  6.6,
                  10.1
                ],
                "items": {
                  "type": "number"
                }
              },
              "address": {
                "type": "string",
                "example": ""
              }
            }
          },
          "isOnline": {
            "type": "boolean",
            "example": false
          },
          "isFeatured": {
            "type": "boolean",
            "example": true
          },
          "isPublish": {
            "type": "boolean",
            "example": false
          }
        },
        "xml": {
          "name": "CreateEventRequest"
        }
      },
