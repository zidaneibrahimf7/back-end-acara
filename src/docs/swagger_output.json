          },
          "email": {
            "type": "string",
            "example": "zidaneibrhmfdl7@gmail.com"
          },
          "password": {
            "type": "string",
            "example": "Ibrhmfdl_1"
          },
          "confirmPassword": {
            "type": "string",
            "example": "Ibrhmfdl_1"
          }
        },
        "xml": {
          "name": "RegisterRequest"
        }
      },
